A1 = float(input('Nota do 1:\n'))
A2 = float(input('Nota do 2:\n'))
A3 = float(input('Nota do 3:\n'))
A4 = float(input('Nota do 4:\n'))

media = (A1+A2+A3+A4)/4
+str()
if media <0 or media > 10:
  print('Valor invalido.')
else:
  if media >=6:
      print('Você foi aprovado, wow!')
  else:
    print('Você foi reprovado, melhor tenta de novo!')
